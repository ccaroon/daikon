{
  "name": "Daikon",
  "codename": "Root Veggie",
  "icon": "fa-carrot",
  "version": "1.0.0a",
  "author": "Craig N. Caroon",
  "buildDate": "September 03, 2022",
  "description": "Sample Electron+Vue+Vuetify App",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ccaroon/daikon"
  },
  "scripts": {
    "build": "vue-cli-service electron:build",
    "build:clean": "rm -rf dist_electron/",
    "serve": "vue-cli-service electron:serve",
    "web:serve": "vue-cli-service serve",
    "web:build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.2",
    "core-js": "^3.8.3",
    "mousetrap": "^1.6.5",
    "vue": "^2.7.8",
    "vue-router": "^3.5.2",
    "vuetify": "^2.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-standard": "^8.0.0",
    "electron": "^20.0.0",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "$electron-builder"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
